 <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
                aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link" id="nav-item" href="index.handlebars">Home</a>
                    <a class="nav-link" id="nav-item" href="savedcampgrounds.handlebars">Favorite Campsites</a>
                    <a class="nav-link" id="nav-item" href="wishlist.handlebars">One Day I'll Camp here!</a>
                </div>
            </div>
        </nav>
    </div>
    <div class="container ">
        <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <h1 class="display-4">My Favorite Campgrounds</h1>
                <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.
                </p>
            </div>
        </div>
    </div>


    <div class="container">
        <ul>
            {{#each campgrounds}}
            {{#if hasVisited}}
        <div class="row">
            <div class="col-12">
                <div class="card text-center">
                    <div class="card-header">
                        {{name}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{location}}</h5>
                        <p class="card-text">{{description}}
                        </p>
                    </div>
                    <div class="card-footer text-muted">
                        {{rating}}
                    </div>
                </div>
            </div>
        </div>
            {{/if}}
            {{/each}}
        </ul>

    </div>

