    <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
                aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link" id="nav-item" href="/">Home</a>
                    <a class="nav-link" id="nav-item" href="/saved">Favorite Campsites</a>
                    <a class="nav-link" id="nav-item" href="/wishlist">One Day I'll Camp here!</a>
                </div>
            </div>
        </nav>
    </div>
    <div class="container-fluid">
        <div class="jumbotron jumbotron-fluid cards">
            <div class="container">
                <h1 class="display-4"></h1>
                <p class="lead"><img  id="logo"src="../assets/images/CampFoundLogo.png" alt="Logo with Tent"></p>
            </div>
        </div>
    </div>
    <ul>
        {{#each campgrounds}}
        {{#unless hasVisited}}
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="card text-center">
                    <div class="card-header">
                        {{name}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{location}}</h5>
                        <p class="card-text">{{description}}
                        </p>
                        <a href="#" class="btn btn-primary">Go somewhere</a>
                    </div>
                    <div class="card-footer text-muted">
                        {{rating}}
                    </div>
                    <button class="delete-btn" data-id="{{id}}">
                         Delete campsite
                    </button>
                </div>
            </div>
        </div>
    </div>
        {{/unless}}
        {{/each}}
    </ul>
